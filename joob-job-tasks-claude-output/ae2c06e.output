          </View>
        </View>

        {/* Languages */}
        <View style={styles.section}>
          <Text style={styles.sectionTitle}>Languages</Text>
          <View style={styles.chipContainer}>
            {LANGUAGES.map((language) => (
              <TouchableOpacity
                key={language}
                style={[
                  styles.selectableChip,
                  formData.languages.includes(language) && styles.selectedChip,
                ]}
                onPress={() => toggleSelection("languages", language)}
              >
                <Text
                  style={[
                    styles.chipText,
                    formData.languages.includes(language) && styles.selectedChipText,
                  ]}
                >
                  {language}
                </Text>
              </TouchableOpacity>
            ))}
          </View>
        </View>

        {/* Traits */}
        <View style={styles.section}>
          <Text style={styles.sectionTitle}>Traits</Text>
          <View style={styles.chipContainer}>
            {TRAITS.map((trait) => (
