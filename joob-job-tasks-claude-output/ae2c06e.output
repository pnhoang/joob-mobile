  "Cleaning",
  "Bartending",
  "Warehouse",
  "Data Entry",
  "Cashier",
  "Cook",
  "Server",
  "Retail",
  "Driver",
  "Security",
  "Maintenance",
  "Administration",
  "Sales",
];

const LANGUAGES = ["German", "English", "Spanish", "French", "Turkish", "Arabic", "Italian"];

const TRAITS = [
